  pod 'UMCShare/Social/WeChat' # 集成微信(完整版14.4M)
  pod 'UMCShare/Social/QQ' # 集成QQ/QZone/TIM(完整版7.6M)
  pod 'UMCShare/Social/Sina' # 集成新浪微博(完整版25.3M)
  pod 'UMCShare/Social/AlipayShare' # 集成支付宝
  # 友盟推送
  pod 'UMCPush'

  # 键盘管理
  pod 'IQKeyboardManager'
  
  # FMDB数据库
  pod 'FMDB'
  # 图片加载
  pod 'SDWebImage'
  
